<?xml version="1.0"?>
<config>
    <modules>
        <Inky_Designersoftware>
            <version>0.1.0</version>
        </Inky_Designersoftware>
    </modules>
    <frontend>
        <routers>
            <designersoftware>
                <use>standard</use>
                <args>
                    <module>Inky_Designersoftware</module>
                    <frontName>designersoftware</frontName>
                </args>
            </designersoftware>
        </routers>
        <layout>
            <updates>
                <designersoftware>
                    <file>inky/inky_designersoftware.xml</file>
                </designersoftware>
            </updates>
        </layout>
		<translate>
			<modules>
				<Inky_Designersoftware>
					<files>
						<default>inky/Inky_Designersoftware.csv</default>
					</files>
				</Inky_Designersoftware>
			</modules>
		</translate>		 
    </frontend>
    <adminhtml>
		<layout>
			<updates>
				<designersoftware>
					<file>inky/inky_designersoftware.xml</file>
				</designersoftware>
			</updates>
		</layout>
    </adminhtml>
    <admin>
        <routers>
			<designersoftware>
				<use>admin</use>
				<args>
					<module>Inky_Designersoftware</module>
					<frontName>designersoftware</frontName>
				</args>
			</designersoftware>
        </routers>
    </admin>     
    <global>
        <models>
            <designersoftware>
                <class>Inky_Designersoftware_Model</class>
                <resourceModel>designersoftware_mysql4</resourceModel>
            </designersoftware>
            <designersoftware_mysql4>
                <class>Inky_Designersoftware_Model_Mysql4</class>
                <entities>
                    <designersoftware>
                        <table>inky_designersoftware</table>
                    </designersoftware>
                    
                    <angles>
                        <table>inky_angles</table>
                    </angles>
                    
                    <color>
                        <table>inky_color</table>
                    </color>
                    
                    <texture>
                        <table>inky_texture</table>
                    </texture>
                    
                    <font>
                        <table>inky_font</table>
                    </font>  
                                      
                    <clipart>
                        <table>inky_clipart</table>
                    </clipart>
                    <clipart_category>
                        <table>inky_clipart_category</table>
                    </clipart_category>
                    <clipart_upload>
                        <table>inky_clipart_upload</table>
                    </clipart_upload>
                    
                   <parts>
                        <table>inky_parts</table>
                    </parts> 
                    <parts_style>
						<table>inky_parts_style</table>
                    </parts_style>                  
                    <parts_layers>
						<table>inky_parts_layers</table>
                    </parts_layers>     
                    <parts_layers_texture>
						<table>inky_parts_layers_texture</table>
                    </parts_layers_texture>                
                                                   
                    <style>
						<table>inky_style</table>
                    </style>               
                    
                    <sizes>
                        <table>inky_sizes</table>
                    </sizes>                    
                </entities>
            </designersoftware_mysql4>
            <sitemap>					
				<rewrite>
					<resource_catalog_product>Inky_Sitemap_Model_Resource_Catalog_Product</resource_catalog_product>
				</rewrite>					
			</sitemap>
        </models>
        <resources>
            <designersoftware_setup>
                <setup>
                    <module>Inky_Designersoftware</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </designersoftware_setup>
            <designersoftware_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </designersoftware_write>
            <designersoftware_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </designersoftware_read>
        </resources>
        <blocks>
            <designersoftware>
                <class>Inky_Designersoftware_Block</class>                
            </designersoftware>
            <!--<catalog>
				<rewrite>
					<product_view_abstract>Inky_Catalog_Block_Product_View_Abstract</product_view_abstract>
				</rewrite>
            </catalog>-->
            <checkout>
				<rewrite>
					<cart_item_renderer>Inky_Checkout_Block_Cart_Item_Renderer</cart_item_renderer>
				</rewrite>
            </checkout>
        </blocks>
        <helpers>
            <designersoftware>
                <class>Inky_Designersoftware_Helper</class>
            </designersoftware>
        </helpers>
    </global>
    
     <!-- Set Admin Template File PAckage and Theme to establish modifications in Admin Templates-->
    <stores>
        <admin>
            <design>
                <package>
                    <name>default</name>
                </package>
                <theme>
                    <default>designersoftware</default>
                </theme>
            </design>
        </admin>
    </stores>    
    <!-- ============================================================== -->
</config>
